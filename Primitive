public class Main {
    public static void main(String[] args) {
        // Define the required primitives
        byte zero = 0;
        char H = 'H';
        int three = 3;
        int one = 1;
        float twoPointZero = 2.0f;
        boolean isTrue = true;

        // Concatenate the primitives into the desired string
        String output = H + "" + three + one + one + zero + " w" + zero + "r" + one + "d " + twoPointZero + " " + isTrue;

        // Print the output
        System.out.println(output);
    }
}
